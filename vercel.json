{
  "version": 2,
  "builds": [
    { "src": "api/paypal/transaction.js", "use": "@vercel/node" },
    { "src": "api/pix/transaction.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/paypal/transaction", "dest": "/api/paypal/transaction.js" },
    { "src": "/api/pix/transaction", "dest": "/api/pix/transaction.js" },
    { "src": "/public/(.*)", "dest": "/public/$1" }
  ]
}
