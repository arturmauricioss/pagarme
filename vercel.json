{
  "version": 2,
  "builds": [
      {
          "src": "api/paypal/transaction.js",
          "use": "@vercel/node"
      },
      {
          "src": "api/pix/transaction.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/api/paypal/transaction",
          "dest": "/api/paypal/transaction.js"
      },
      {
          "src": "/api/pix/qr-code",
          "dest": "/api/pix/transaction.js"
      }
  ]
}
