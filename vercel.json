{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/paypal/(.*)",
      "dest": "/api/paypal/transaction.js"
    },
    {
      "src": "/api/pix/(.*)",
      "dest": "/api/pix/transaction.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
